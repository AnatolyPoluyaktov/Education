   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "5.6"
      ]
     },
     "execution_count": 28,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "\n",
    "s=set(lst)\n",
    "lst=list(lst)\n",
    "d=dict()\n",
    "for i in s:\n",
    "    d[i]=lst.count(i)\n",
    "ans=0\n",
    "for key, value in d.items():\n",
    "    ans+=key*value\n",
    "ans/len(lst)"
   ]
  }
